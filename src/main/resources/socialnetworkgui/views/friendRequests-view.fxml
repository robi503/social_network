<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="socialnetworkgui.FriendController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TableView fx:id="tableView" minHeight="-Infinity" minWidth="-Infinity" prefHeight="381.0" prefWidth="663.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn fx:id="tableColumnId1" prefWidth="75.0" text="Id1" />
                <TableColumn fx:id="tableColumnId2" prefWidth="75.0" text="Id2" />
                <TableColumn fx:id="tableColumnFriendsFrom" prefWidth="75.0" text="Sent at" />
                <TableColumn fx:id="tableColumnPending" prefWidth="75.0" text="Pending" />
                <TableColumn fx:id="tableColumnAccepted" prefWidth="75.0" text="Accepted" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
    <HBox prefHeight="34.0" prefWidth="655.0" spacing="20.0" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="0.0">
        <Button mnemonicParsing="false" onAction="#handleAccept" text="Accept" />
        <Button mnemonicParsing="false" onAction="#handleDeny" text="Deny" />
        </HBox>
    </children>

</AnchorPane>
